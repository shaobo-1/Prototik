{
    "data": {
        "Holo.Theme | HoloBase BaseDark BaseBarDark < Theme.Sherlock": {
            "holoTheme": "dark"
        },
        "Holo.Theme.Light | HoloBase BaseLight BaseBarLight < Theme.Sherlock.Light": {
            "holoTheme": "light"
        },
        "Holo.Theme.Light.DarkActionBar | HoloBase BaseLight BaseBarDark < Theme.Sherlock.Light.DarkActionBar": {
            "holoTheme": "mixed"
        }
    },
    "blocks": {
    	"HoloBase": {
            "textAppearanceLargePopupMenu": "?android:textAppearanceLargePopupMenu",
            "textAppearanceSmallPopupMenu": "?android:textAppearanceSmallPopupMenu"
    	}
    },
    "include": [
        {
            "name": "styles-core.json:styles",
            "type": "only blocks"
        }
    ],
    "output": "values-v14/styles.xml"
}
