{
    "blocks": {
        "Holo": {
            "textAppearanceLargePopupMenu": "?android:textAppearanceLargePopupMenu",
            "textAppearanceSmallPopupMenu": "?android:textAppearanceSmallPopupMenu"
        },
        "HoloBar": {},
        "HoloBarDark | HoloBar BarDark": {
            "android:textAppearanceLargePopupMenu": "@style/Holo.TextAppearance",
            "android:textAppearanceSmallPopupMenu": "@style/Holo.TextAppearance.Small"
        },
        "HoloBarLight | HoloBar BarLight": {
            "android:textAppearanceLargePopupMenu": "@style/Holo.TextAppearance.Light",
            "android:textAppearanceSmallPopupMenu": "@style/Holo.TextAppearance.Small.Light"
        },
        "HoloDark | Holo Dark": {},
        "HoloLight | Holo Light": {}
    },
    "data": {
        "Holo.Theme | HoloDark HoloBarDark < Theme.Sherlock": {
            "holoTheme": "dark"
        },
        "Holo.Theme.Light | HoloLight HoloBarLight < Theme.Sherlock.Light": {
            "holoTheme": "light"
        },
        "Holo.Theme.Light.DarkActionBar | HoloLight HoloBarDark < Theme.Sherlock.Light.DarkActionBar": {
            "holoTheme": "mixed"
        }
    },
    "include": [
        "themes-core.json:styles"
    ],
    "output": "values-v14/themes.xml"
}
