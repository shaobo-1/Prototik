<?xml version="1.0" encoding="utf-8"?><resources>
    <!--Start attrs definition block-->
    <attr name="dragBound" format="integer|fraction|reference|dimension"/>
    <attr name="leftDragBound" format="integer|fraction|reference|dimension"/>
    <attr name="leftShadow" format="float|fraction|reference|dimension"/>
    <attr name="leftShadowColor" format="color|reference"/>
    <attr name="leftTranslateFactor" format="float|fraction|reference|dimension"/>
    <attr name="rightDragBound" format="integer|fraction|reference|dimension"/>
    <attr name="rightShadow" format="float|fraction|reference|dimension"/>
    <attr name="rightShadowColor" format="color|reference"/>
    <attr name="rightTranslateFactor" format="float|fraction|reference|dimension"/>
    <attr name="scrollInterpolator" format="reference"/>
    <attr name="shadow" format="float|fraction|reference|dimension"/>
    <attr name="shadowColor" format="color|reference"/>
    <attr name="shadowInterpolator" format="reference"/>
    <attr name="sliderStyle" format="reference"/>
    <attr name="touchMode" format="enum">
        <enum name="fullscreen" value="4"/>
        <enum name="left" value="1"/>
        <enum name="leftright" value="3"/>
        <enum name="none" value="0"/>
        <enum name="right" value="2"/>
    </attr>
    <attr name="touchModeLeftMargin" format="reference|dimension"/>
    <attr name="touchModeMargin" format="reference|dimension"/>
    <attr name="touchModeRightMargin" format="reference|dimension"/>
    <attr name="translateFactor" format="float|fraction|reference|dimension"/>
    <attr name="translateInterpolator" format="reference"/>
    <!--Start styleable definition block-->
    <declare-styleable name="Slider">
        <attr name="dragBound"/>
        <attr name="leftDragBound"/>
        <attr name="leftShadow"/>
        <attr name="leftShadowColor"/>
        <attr name="leftTranslateFactor"/>
        <attr name="rightDragBound"/>
        <attr name="rightShadow"/>
        <attr name="rightShadowColor"/>
        <attr name="rightTranslateFactor"/>
        <attr name="scrollInterpolator"/>
        <attr name="shadow"/>
        <attr name="shadowColor"/>
        <attr name="shadowInterpolator"/>
        <attr name="touchMode"/>
        <attr name="touchModeLeftMargin"/>
        <attr name="touchModeMargin"/>
        <attr name="touchModeRightMargin"/>
        <attr name="translateFactor"/>
        <attr name="translateInterpolator"/>
    </declare-styleable>
    <!--End of any blocks-->
</resources>
